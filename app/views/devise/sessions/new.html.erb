<div id="main">
  <h1 class="login_title">Hi</h1>
  <h1 class="login_title">Mini Job Seeker</h1>
      <%= image_tag("logIn-seeker.png") %>
  <div id="login_input">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="input"><%= f.label :email %><br />
    <%= f.email_field :email, class: "input", autofocus: true %></div>

    <div ><%= f.label :password %><br />
      <%= f.password_field :password, autocomplete: "off", class: "input" %></div>

    <div id="submit"><%= f.submit "Sign in" ,id: "submit" %></div><!-- link to dashboard (kid, parent, adult) -->
  <% end %>

<!--   < %= render "devise/shared/links" %> -->
</div>